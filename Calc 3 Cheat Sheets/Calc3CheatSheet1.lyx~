\begin_layout Subsubsection*
Dot Product:
\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}\cdot\vec{b}=|\vec{a}||\vec{b}|cos\theta$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}=<a_{1},a_{2},a_{3}>$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{b}=<b_{1},b_{2},b_{3}>$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}\cdot\vec{b}=a_{1}b_{1}+a_{2}b_{2}+a_{3}b_{3}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}\cdot\vec{b}=0,\vec{a}\perp\vec{b}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
columnbreak
\end_layout

\end_inset


\end_layout

\begin_layout Subsubsection*
Cross Product:
\end_layout

\begin_layout Standard
\begin_inset Formula $|\vec{a}\times\vec{b}|=|\vec{a}||\vec{b}|sin\theta$
\end_inset

 OR
\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}\times\vec{b}=\begin{array}{ccc}
\vec{i} & \vec{j} & \vec{k}\\
a_{1} & a_{2} & a_{3}\\
b_{1} & b_{2} & b_{3}
\end{array}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $=(a_{2}b_{3}-a_{3}b_{2})\vec{i}-(a_{1}b_{3}-a_{3}b_{1})\vec{j}-(a_{1}b_{2}-a_{2}b_{1})\vec{k}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{u}\cdot(\vec{v}\times\vec{w})=\begin{array}{ccc}
u_{1} & u_{2} & u_{3}\\
v_{1} & v_{2} & v_{3}\\
w_{1} & w_{2} & w_{3}
\end{array}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}\times\vec{b}=\vec{0},\vec{a}\parallel\vec{b}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
columnbreak
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Area of a Parellelogram: 
\begin_inset Formula $|\vec{a}\times\vec{b}|$
\end_inset


\end_layout

\begin_layout Standard
Volume of a Parallelepiped: 
\begin_inset Formula $|\vec{a}\cdot(\vec{b}\times\vec{c})|$
\end_inset


\end_layout

\begin_layout Standard
Scalar projection of 
\begin_inset Formula $\vec{b}$
\end_inset

 onto 
\begin_inset Formula $\vec{a}$
\end_inset

: 
\begin_inset Formula $Comp_{\vec{a}}\vec{b}=\frac{\vec{a}\cdot\vec{b}}{|\vec{a}|}$
\end_inset


\end_layout

\begin_layout Standard
Vector projection of 
\begin_inset Formula $\vec{b}$
\end_inset

 onto 
\begin_inset Formula $\vec{a}$
\end_inset

: 
\begin_inset Formula $Proj_{\vec{a}}\vec{b}=\frac{\vec{a}\cdot\vec{b}}{|\vec{a}|^{2}}(\vec{a})$
\end_inset


\end_layout

