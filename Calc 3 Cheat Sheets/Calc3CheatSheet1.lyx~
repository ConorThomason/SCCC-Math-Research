\end_layout

\begin_layout Standard
Ellipsoid: 
\begin_inset Formula $\frac{x^{2}}{a^{2}}+\frac{y^{2}}{b^{2}}+\frac{z^{2}}{c^{2}}=1$
\end_inset


\end_layout

\begin_layout Standard
Elliptic Paraboloid: 
\begin_inset Formula $\frac{z}{c}-\frac{x^{2}}{a^{2}}+\frac{y^{2}}{b^{2}}$
\end_inset


\end_layout

\begin_layout Standard
Hyperbolic Paraboloid: 
\begin_inset Formula $\frac{z}{c}=\frac{x^{2}}{a^{2}}-\frac{y^{2}}{b^{2}}$
\end_inset


\end_layout

\begin_layout Standard
Cone: 
\begin_inset Formula $\frac{z^{2}}{c^{2}}=\frac{x^{2}}{a^{2}}+\frac{y^{2}}{b^{2}}$
\end_inset


\end_layout

\begin_layout Standard
Hyperboloid of One Sheet: 
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{x^{2}}{a^{2}}+\frac{y^{2}}{b^{2}}-\frac{z^{2}}{c^{2}}=1$
\end_inset


\end_layout

\begin_layout Standard
Hyperboloid of Two Sheets:
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{-z^{2}}{a^{2}}-\frac{y^{2}}{b^{2}}+\frac{z^{2}}{c^{2}}=1$
\end_inset


\end_layout

