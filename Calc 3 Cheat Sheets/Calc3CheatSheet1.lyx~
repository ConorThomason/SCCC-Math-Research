\begin_layout Subsubsection*
Vector Calculus:
\end_layout

\begin_layout Standard
\begin_inset Formula $\underset{t\rightarrow a}{lim}\vec{r}(t)=<\text{\ensuremath{\underset{t\rightarrow a}{lim}f(t),\underset{t\rightarrow a}{lim}g(t),\underset{t\rightarrow a}{lim}h(t)>}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{r}'(t)=<f'(t),g'(t),h'(t)>=f'(t)\vec{i}+g'(t)\vec{j}+h'(t)\vec{k}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{d}{dt}(\vec{u}+\vec{v})=\vec{u}'+\vec{v}'$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $(c\vec{u})'=c\vec{u}'$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{d}{dt}(f(t)\vec{u}(t))=f'(t)\vec{u}(t)+f(t)\vec{u}'$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{d}{dt}(\vec{u}\cdot\vec{v})=\vec{u}'\cdot\vec{v}+\vec{u}\cdot\vec{v}'$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{d}{dt}(\vec{u}\times\vec{v})=\vec{u'}\times\vec{v}+\vec{u}\times\vec{v}'$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{d}{dt}(\vec{u}(f(t))=f'(t)\vec{u}'(f(t))$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
columnbreak
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\int\vec{r}(t)dt=<\int f(t)dt,\int g(t)dt,\int h(t)dt>+\vec{c}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\int\vec{r}(t)dt=\int f(t)dt\vec{i}+\int g(t)dt\vec{j}+\int h(t)dt\vec{k}+\vec{c}$
\end_inset


\end_layout

