\begin_layout Subsubsection*
Cross Product:
\end_layout

\begin_layout Standard
\begin_inset Formula $|\vec{a}\times\vec{b}|=|\vec{a}||\vec{b}|sin\theta$
\end_inset

 OR
\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}\times\vec{b}=\begin{array}{ccc}
\vec{i} & \vec{j} & \vec{k}\\
a_{1} & a_{2} & a_{3}\\
b_{1} & b_{2} & b_{3}
\end{array}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{u}\cdot(\vec{v}\times\vec{w})=\begin{array}{ccc}
u_{1} & u_{2} & u_{3}\\
v_{1} & v_{2} & v_{3}\\
w_{1} & w_{2} & w_{3}
\end{array}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{a}\times\vec{b}=\vec{0},\vec{a}\parallel\vec{b}$
\end_inset


\end_layout

