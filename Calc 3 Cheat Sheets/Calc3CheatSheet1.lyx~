\begin_layout Subsubsection*
Angle Between Planes:
\end_layout

\begin_layout Standard
\begin_inset Formula $cos\theta=\frac{n_{1}\cdot n_{2}}{|n_{1}||n_{2}|}$
\end_inset


\end_layout

\begin_layout Standard
Distance from Point to Plane: 
\end_layout

\begin_layout Standard
\begin_inset Formula $D=\frac{|n\cdot b|}{|n|}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{b}=<x_{1}-x_{0},y_{1}-y_{0},z_{1}-z_{0}>$
\end_inset


