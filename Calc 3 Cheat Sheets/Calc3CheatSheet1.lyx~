\begin_layout Standard
Arc Length: 
\begin_inset Formula $L=\stackrel[a]{b}{\int}|r'(t)|dt$
\end_inset


\end_layout

\begin_layout Standard
Unit Tangent Vector: 
\begin_inset Formula $\vec{T}(t)=\frac{\vec{r}'(t)}{|\vec{r}'(t)'|}$
\end_inset


\end_layout

\begin_layout Standard
Curvature 
\begin_inset Formula $\kappa(t)=\frac{|\vec{T}'(t)|}{|\vec{r}'(t)|}$
\end_inset


\end_layout

\begin_layout Standard
Unit Normal Vector: 
\begin_inset Formula $\vec{N}(t)=\frac{\vec{T}'(t)}{|\vec{T}'(t)|}$
\end_inset


\end_layout

\begin_layout Standard
Binormal Vector: 
\begin_inset Formula $\vec{B}(t)=\vec{T}(t)\times\vec{N}(t)$
\end_inset


\end_layout

\begin_layout Standard
Velocity: 
\begin_inset Formula $r'(t)$
\end_inset

 Acceleration: 
\begin_inset Formula $r''(t)$
\end_inset

 Speed: 
\begin_inset Formula $|r'(t)|$
\end_inset


