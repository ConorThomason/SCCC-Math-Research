\begin_layout Subsubsection*
Vector Equation:
\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{r}=\vec{r_{0}}+t\vec{v}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\vec{r}=<x,y,z>$
\end_inset


\end_layout

\begin_layout Standard
